  <div class="col-lg-12">
                      <div class="panel panel-default">
                          <div class="panel-heading">
                              C-2: Meal Description
                          </div>
                          <!-- /.panel-heading -->
                          <div class="panel-body">
                              <table width="100%" class="table table-striped table-bordered table-hover" id="dataTables-example">
                                  <thead>
                                      <tr>
                                          <th>Date</th>
                                          <th>Shift Captain</th>
                                          <th>Meal Destination</th>
                                          <th>Servings</th>
                                          <th>Congr/Indiv</th>
                                          <th>MC1: Protein</th>
                                          <th>MC1: Starch/Grain</th>
                                          <th>MC1: Vegetable</th>
                                          <th>MC1: Fruit/Desert</th>
                                          <th>MC2: Protein </th>
                                          <th>MC2: Starch/Grain </th>
                                          <th>MC2: Vegetable </th>
                                          <th>MC2: Fruit/Dessert </th>
                                          <th>Out of Cooler: Time</th>
                                          <th>Out of Cooler: Temp</th>
                                          <th>Initials</th>
                                      </tr>
                                  </thead>
                                  <tbody>
                                    <% for(var i=0; i<records.length; i++) {%>
                                      <tr>

                                        <td>
                                          <div class="form-group">
                                              <input type="date" class="form-control" name="<%= 'date'+i %>" readonly value="<%= records[i].date %>">
                                          </div>
                                        </td>


                                        <td>
                                          <div class="form-group">
                                              <input type="shifcap" class="form-control" name="<%= 'shifcap'+i %>" readonly value="<%= records[i].shifcap %>">
                                          </div>
                                        </td>

                                        <td>
                                          <div class="form-group">
                                              <input type="mealdest" class="form-control" name="<%= 'mealdest'+i %>" readonly value="<%= records[i].mealdest %>">
                                          </div>
                                        </td>

                                        <td>
                                          <div class="form-group">
                                              <input type="servings" class="form-control" name="<%= 'servings'+i %>" readonly value="<%= records[i].servings %>">
                                          </div>
                                        </td>

                                        <td>
                                          <div class="form-group">
                                              <input type="congind" class="form-control" name="<%= 'congind'+i %>" readonly value="<%= records[i].congind %>">
                                          </div>
                                        </td>

                                        <td>
                                          <div class="form-group">
                                              <input type="mc1prot" class="form-control" name="<%= 'mc1prot'+i %>" readonly value="<%= records[i].mc1prot %>">
                                          </div>
                                        </td>

                                        <td>
                                          <div class="form-group">
                                              <input type="mc1stgr" class="form-control" name="<%= 'mc1stgr'+i %>" readonly value="<%= records[i].mc1stgr %>">
                                          </div>
                                        </td>

                                        <td>
                                          <div class="form-group">
                                              <input type="mc1veg" class="form-control" name="<%= 'mc1veg'+i %>" readonly value="<%= records[i].mc1veg %>">
                                          </div>
                                        </td>

                                        <td>
                                          <div class="form-group">
                                              <input type="mc1frde" class="form-control" name="<%= 'mc1frde'+i %>" readonly value="<%= records[i].mc1frde %>">
                                          </div>
                                        </td>

                                        <td>
                                          <div class="form-group">
                                              <input type="mc2prot" class="form-control" name="<%= 'mc2prot'+i %>" readonly value="<%= records[i].mc2prot %>">
                                          </div>
                                        </td>

                                        <td>
                                          <div class="form-group">
                                              <input type="mc2stgr" class="form-control" name="<%= 'mc2stgr'+i %>" readonly value="<%= records[i].mc2stgr %>">
                                          </div>
                                        </td>

                                        <td>
                                          <div class="form-group">
                                              <input type="mc2veg" class="form-control" name="<%= 'mc2veg'+i %>" readonly value="<%= records[i].mc2veg %>">
                                          </div>
                                        </td>

                                        <td>
                                          <div class="form-group">
                                              <input type="mc2frde" class="form-control" name="<%= 'mc2frde'+i %>" readonly value="<%= records[i].mc2frde %>">
                                          </div>
                                        </td>

                                        <td>
                                          <div class="form-group">
                                              <input type="ooctime" class="form-control" name="<%= 'ooctime'+i %>" readonly value="<%= records[i].ooctime %>">
                                          </div>
                                        </td>

                                        <td>
                                          <div class="form-group">
                                              <input type="ooctemp" class="form-control" name="<%= 'ooctemp'+i %>" readonly value="<%= records[i].ooctemp %>">
                                          </div>
                                        </td>

                                        <td>
                                          <div class="form-group">
                                            <input type="initials" class="form-control" name="<%= 'initials'+i %>" readonly value="<%= records[i].initials %>">
                                        </td>



                                        <!--
                                        <td><span>
                                          <input type="hidden" name="unique_id" value="<%= records[i]._id %>">
                                          <button type="button" class="btn btn-primary"id=<%='edit'+i%> onClick="edit(this)">Edit</button></form></span></td>
                                        <td><span><form id="<%='newReq'+i%>" action="/remove" method="POST">
                                          <input type="hidden" name="num" value="<%= i %>">
                                          <button type="button" class="btn btn-danger" id=<%='delete'+i%> onClick="deleteEntry(this)">Delete</button></form></span></td>
                                        -->
                                      </tr>
                                    <% } %>
                                  </tbody>
                              </table>
                              <!-- /.table-responsive -->
                          </div>
                          <!-- /.panel-body -->
                      </div>
                      <!-- /.panel -->
          </div>

          <!-- jQuery -->
          <script src="../jquery/jquery.min.js"></script>

              <!-- DataTables JavaScript -->
          <script src="../datatables/js/jquery.dataTables.min.js"></script>
          <script src="../datatables-plugins/dataTables.bootstrap.min.js"></script>
          <script src="../datatables-responsive/dataTables.responsive.js"></script>
